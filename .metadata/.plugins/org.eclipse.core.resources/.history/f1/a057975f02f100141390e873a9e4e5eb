#include <stdio.h>
#include <string.h>

int buscarComando();

typedef struct comandos{
	char* comando;

}comandos;
comandos listaComandos [10];

int r, cont = 0, r1;

int main(){

char* comando;



listaComandos [1].comando="formatear";

scanf("%s",comando);
r1 = int buscarComando(comando);
printf("%d",r1);


return 0;
}

int buscarComando(char* a){

	do{
		r = strcmp(a, listaComandos[cont].comando);
		cont = cont + 1;
	}while((cont>30) || (r == 0));

	if (cont>30){
		printf("no existe el comando");
		return 0;
	}

return cont;
}
