#include <stdio.h>
#include <string.h>
int buscarComando();

int r, cont = 0;
int main(){
typedef struct comandos{
	char* comando;

}comandos;
char* comando;
comandos listaComandos [10];


listaComandos [1].comando="formatear";

scanf("%s",comando);
int buscarComando(comando);


return 0;
}

int buscarComando(char* a){

	do{
		r = strcmp(a, listaComandos [1].comando);
		cont = cont + 1;
	}while((cont>30) || (r == 0));

	if (cont>30){
		printf("no existe el comando");
		return 0;
	}

return cont;
}
